{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://geojson.org/schema/FeatureCollection.json",
    "title": "GeoJSON FeatureCollection",
    "type": "object",
    "required": [
        "type",
        "@context",
        "features"
    ],
    "properties": {
        "type": {
            "type": "string",
            "enum": [
                "FeatureCollection"
            ]
        },
        "@context": {
            "title": "LinkedPasts extended property: '@context'",
            "description": "In JSON-LD, labels for object elements are aliases for terms formally defined in linked ontologies. For Linked Places, those mappings are listed in this context document.",
            "type": "string",
            "enum": [
                "https://raw.githubusercontent.com/isawnyu/linked-places/main/linkedplaces-context-v1.1.jsonld"
            ]
        },
        "features": {
            "type": "array",
            "items": {
                "title": "GeoJSON Feature",
                "type": "object",
                "required": [
                    "@id",
                    "type",
                    "properties",
                    "geometry"
                ],
                "properties": {
                    "@id": {
                        "title": "LinkedPlaces extended property: '@id'",
                        "description": "A unique and permanent URI pointing to the contributor's published record of the place.",
                        "type": "string",
                        "pattern": "^http[s]?://(?:[a-zA-Z]|[0-9]|[$-_@.&+]|[!*\\(\\),]|(?:%[0-9a-fA-F][0-9a-fA-F]))+$"
                    },
                    "type": {
                        "type": "string",
                        "enum": [
                            "Feature"
                        ]
                    },
                    "id": {
                        "oneOf": [{
                                "type": "number"
                            },
                            {
                                "type": "string"
                            }
                        ]
                    },
                    "properties": {
                        "title": "LinkedPlaces extends the GeoJSON 'properties' object",
                        "description": "A properties element holding at least one key:value pair is required by GeoJSON. For Linked Places format, title is required and one or more ccodes are encouraged.",
                        "type": "object",
                        "required": [
                            "title"
                        ],
                        "properties": {
                            "title": {
                                "title": "LinkedPlaces requires a title string.",
                                "type": "string",
                                "pattern": "^.*[^\\s]+.*$"
                            },
                            "ccode": {
                                "title": "LinkedPlaces encourages country codes.",
                                "description": "Assumption: LinkedPlaces expects values from the ISO 3166 Alpha-2, Alpha-3, or three-digit 'numeric' code lists. E.g., 'AF', 'AFG', or '004' for 'Afghanistan'.",
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "pattern": "^([A-Z]{2,3}|\\d{3})$"
                                }
                            }
                        }
                    },
                    "when": {
                        "title": "LinkedPlaces extended property 'when'",
                        "description": "A 'when' element can be used to temporally scope an entire Feature",
                        "type": "object",
                        "required": [
                            "timespans"
                        ],
                        "properties": {
                            "timespans": {
                                "title": "LinkedPlaces extended property 'timespans'",
                                "type": "array",
                                "minItems": 1,
                                "items": {
                                    "title": "LinkedPlaces extended property 'timespan'",
                                    "description": "A 'timespan' must have a start; if end is omitted, the timespan is interpreted as the interval described by the start.",
                                    "type": "object",
                                    "required": [
                                        "start"
                                    ],
                                    "properties": {
                                        "start": {
                                            "title": "LinkedPlaces extension: 'start'",
                                            "description": "Valid values for 'in', 'earliest', and 'latest' are ISO 8601 expressions as described by the[OWL - Time ontology](https: //www.w3.org/TR/owl-time/)",
                                            "type": "object",
                                            "items": {
                                                "type": "object",
                                                "anyOf": [{
                                                        "required": [
                                                            "in"
                                                        ]
                                                    },
                                                    {
                                                        "required": [
                                                            "earliest"
                                                        ]
                                                    },
                                                    {
                                                        "required": [
                                                            "latest"
                                                        ]
                                                    }
                                                ],
                                                "properties": {
                                                    "in": {
                                                        "type": "string",
                                                        "format": "date"
                                                    },
                                                    "earliest": {
                                                        "type": "string",
                                                        "format": "date"
                                                    },
                                                    "latest": {
                                                        "type": "string",
                                                        "format": "date"
                                                    }
                                                }
                                            }
                                        }
                                    },
                                    "end": {}
                                }
                            }
                        },
                        "periods": {},
                        "label": {},
                        "duration": {}
                    }
                },
                "geometry": {
                    "oneOf": [{
                            "type": "null"
                        },
                        {
                            "title": "GeoJSON Point",
                            "type": "object",
                            "required": [
                                "type",
                                "coordinates"
                            ],
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "enum": [
                                        "Point"
                                    ]
                                },
                                "coordinates": {
                                    "type": "array",
                                    "minItems": 2,
                                    "items": {
                                        "type": "number"
                                    }
                                },
                                "bbox": {
                                    "type": "array",
                                    "minItems": 4,
                                    "items": {
                                        "type": "number"
                                    }
                                }
                            }
                        },
                        {
                            "title": "GeoJSON LineString",
                            "type": "object",
                            "required": [
                                "type",
                                "coordinates"
                            ],
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "enum": [
                                        "LineString"
                                    ]
                                },
                                "coordinates": {
                                    "type": "array",
                                    "minItems": 2,
                                    "items": {
                                        "type": "array",
                                        "minItems": 2,
                                        "items": {
                                            "type": "number"
                                        }
                                    }
                                },
                                "bbox": {
                                    "type": "array",
                                    "minItems": 4,
                                    "items": {
                                        "type": "number"
                                    }
                                }
                            }
                        },
                        {
                            "title": "GeoJSON Polygon",
                            "type": "object",
                            "required": [
                                "type",
                                "coordinates"
                            ],
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "enum": [
                                        "Polygon"
                                    ]
                                },
                                "coordinates": {
                                    "type": "array",
                                    "items": {
                                        "type": "array",
                                        "minItems": 4,
                                        "items": {
                                            "type": "array",
                                            "minItems": 2,
                                            "items": {
                                                "type": "number"
                                            }
                                        }
                                    }
                                },
                                "bbox": {
                                    "type": "array",
                                    "minItems": 4,
                                    "items": {
                                        "type": "number"
                                    }
                                }
                            }
                        },
                        {
                            "title": "GeoJSON MultiPoint",
                            "type": "object",
                            "required": [
                                "type",
                                "coordinates"
                            ],
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "enum": [
                                        "MultiPoint"
                                    ]
                                },
                                "coordinates": {
                                    "type": "array",
                                    "items": {
                                        "type": "array",
                                        "minItems": 2,
                                        "items": {
                                            "type": "number"
                                        }
                                    }
                                },
                                "bbox": {
                                    "type": "array",
                                    "minItems": 4,
                                    "items": {
                                        "type": "number"
                                    }
                                }
                            }
                        },
                        {
                            "title": "GeoJSON MultiLineString",
                            "type": "object",
                            "required": [
                                "type",
                                "coordinates"
                            ],
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "enum": [
                                        "MultiLineString"
                                    ]
                                },
                                "coordinates": {
                                    "type": "array",
                                    "items": {
                                        "type": "array",
                                        "minItems": 2,
                                        "items": {
                                            "type": "array",
                                            "minItems": 2,
                                            "items": {
                                                "type": "number"
                                            }
                                        }
                                    }
                                },
                                "bbox": {
                                    "type": "array",
                                    "minItems": 4,
                                    "items": {
                                        "type": "number"
                                    }
                                }
                            }
                        },
                        {
                            "title": "GeoJSON MultiPolygon",
                            "type": "object",
                            "required": [
                                "type",
                                "coordinates"
                            ],
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "enum": [
                                        "MultiPolygon"
                                    ]
                                },
                                "coordinates": {
                                    "type": "array",
                                    "items": {
                                        "type": "array",
                                        "items": {
                                            "type": "array",
                                            "minItems": 4,
                                            "items": {
                                                "type": "array",
                                                "minItems": 2,
                                                "items": {
                                                    "type": "number"
                                                }
                                            }
                                        }
                                    }
                                },
                                "bbox": {
                                    "type": "array",
                                    "minItems": 4,
                                    "items": {
                                        "type": "number"
                                    }
                                }
                            }
                        },
                        {
                            "title": "GeoJSON GeometryCollection",
                            "type": "object",
                            "required": [
                                "type",
                                "geometries"
                            ],
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "enum": [
                                        "GeometryCollection"
                                    ]
                                },
                                "geometries": {
                                    "type": "array",
                                    "items": {
                                        "oneOf": [{
                                                "title": "GeoJSON Point",
                                                "type": "object",
                                                "required": [
                                                    "type",
                                                    "coordinates"
                                                ],
                                                "properties": {
                                                    "type": {
                                                        "type": "string",
                                                        "enum": [
                                                            "Point"
                                                        ]
                                                    },
                                                    "coordinates": {
                                                        "type": "array",
                                                        "minItems": 2,
                                                        "items": {
                                                            "type": "number"
                                                        }
                                                    },
                                                    "bbox": {
                                                        "type": "array",
                                                        "minItems": 4,
                                                        "items": {
                                                            "type": "number"
                                                        }
                                                    }
                                                }
                                            },
                                            {
                                                "title": "GeoJSON LineString",
                                                "type": "object",
                                                "required": [
                                                    "type",
                                                    "coordinates"
                                                ],
                                                "properties": {
                                                    "type": {
                                                        "type": "string",
                                                        "enum": [
                                                            "LineString"
                                                        ]
                                                    },
                                                    "coordinates": {
                                                        "type": "array",
                                                        "minItems": 2,
                                                        "items": {
                                                            "type": "array",
                                                            "minItems": 2,
                                                            "items": {
                                                                "type": "number"
                                                            }
                                                        }
                                                    },
                                                    "bbox": {
                                                        "type": "array",
                                                        "minItems": 4,
                                                        "items": {
                                                            "type": "number"
                                                        }
                                                    }
                                                }
                                            },
                                            {
                                                "title": "GeoJSON Polygon",
                                                "type": "object",
                                                "required": [
                                                    "type",
                                                    "coordinates"
                                                ],
                                                "properties": {
                                                    "type": {
                                                        "type": "string",
                                                        "enum": [
                                                            "Polygon"
                                                        ]
                                                    },
                                                    "coordinates": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "array",
                                                            "minItems": 4,
                                                            "items": {
                                                                "type": "array",
                                                                "minItems": 2,
                                                                "items": {
                                                                    "type": "number"
                                                                }
                                                            }
                                                        }
                                                    },
                                                    "bbox": {
                                                        "type": "array",
                                                        "minItems": 4,
                                                        "items": {
                                                            "type": "number"
                                                        }
                                                    }
                                                }
                                            },
                                            {
                                                "title": "GeoJSON MultiPoint",
                                                "type": "object",
                                                "required": [
                                                    "type",
                                                    "coordinates"
                                                ],
                                                "properties": {
                                                    "type": {
                                                        "type": "string",
                                                        "enum": [
                                                            "MultiPoint"
                                                        ]
                                                    },
                                                    "coordinates": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "array",
                                                            "minItems": 2,
                                                            "items": {
                                                                "type": "number"
                                                            }
                                                        }
                                                    },
                                                    "bbox": {
                                                        "type": "array",
                                                        "minItems": 4,
                                                        "items": {
                                                            "type": "number"
                                                        }
                                                    }
                                                }
                                            },
                                            {
                                                "title": "GeoJSON MultiLineString",
                                                "type": "object",
                                                "required": [
                                                    "type",
                                                    "coordinates"
                                                ],
                                                "properties": {
                                                    "type": {
                                                        "type": "string",
                                                        "enum": [
                                                            "MultiLineString"
                                                        ]
                                                    },
                                                    "coordinates": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "array",
                                                            "minItems": 2,
                                                            "items": {
                                                                "type": "array",
                                                                "minItems": 2,
                                                                "items": {
                                                                    "type": "number"
                                                                }
                                                            }
                                                        }
                                                    },
                                                    "bbox": {
                                                        "type": "array",
                                                        "minItems": 4,
                                                        "items": {
                                                            "type": "number"
                                                        }
                                                    }
                                                }
                                            },
                                            {
                                                "title": "GeoJSON MultiPolygon",
                                                "type": "object",
                                                "required": [
                                                    "type",
                                                    "coordinates"
                                                ],
                                                "properties": {
                                                    "type": {
                                                        "type": "string",
                                                        "enum": [
                                                            "MultiPolygon"
                                                        ]
                                                    },
                                                    "coordinates": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "array",
                                                            "items": {
                                                                "type": "array",
                                                                "minItems": 4,
                                                                "items": {
                                                                    "type": "array",
                                                                    "minItems": 2,
                                                                    "items": {
                                                                        "type": "number"
                                                                    }
                                                                }
                                                            }
                                                        }
                                                    },
                                                    "bbox": {
                                                        "type": "array",
                                                        "minItems": 4,
                                                        "items": {
                                                            "type": "number"
                                                        }
                                                    }
                                                }
                                            }
                                        ]
                                    }
                                },
                                "bbox": {
                                    "type": "array",
                                    "minItems": 4,
                                    "items": {
                                        "type": "number"
                                    }
                                }
                            }
                        }
                    ]
                },
                "bbox": {
                    "type": "array",
                    "minItems": 4,
                    "items": {
                        "type": "number"
                    }
                }
            }
        }
    },
    "bbox": {
        "type": "array",
        "minItems": 4,
        "items": {
            "type": "number"
        }
    }
}
}